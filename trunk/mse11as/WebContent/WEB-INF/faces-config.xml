<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>do</from-outcome>
			<to-view-id>/pages/test-materials/filltest_questions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
	<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/pages/test-materials/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ReadyTest</from-outcome>
			<to-view-id>/pages/test-materials/public/selecttest.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
		<navigation-rule>
	<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>GenerateMyTest</from-outcome>
			<to-view-id>/pages/test-materials/public/fillquestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
 <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>CancelCreateTest</from-outcome>
   <to-view-id>/pages/test-materials/createtest.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 	<navigation-rule>
 <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>GroupComplete</from-outcome>
   <to-view-id>/pages/administration/index.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>

 <navigation-rule>
 <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>GroupDeleted</from-outcome>
   <to-view-id>/pages/administration/editgroup.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
  	<navigation-rule>
 <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>GroupForEdit</from-outcome>
   <to-view-id>/pages/administration/addgroup.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>

<navigation-rule>
 <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>del</from-outcome>
   <to-view-id>/pages/test-materials/edittest.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
  
 <navigation-rule>
   <from-view-id>*</from-view-id>
   <navigation-case>
       <from-outcome>next</from-outcome>
       <to-view-id>/pages/attestation/createNewAttestationQuestions.xhtml</to-view-id>
   </navigation-case>
</navigation-rule>
</faces-config>
